{
  "name" : "vempain-file-frontend",
  "private" : true,
  "version" : "0.1.0",
  "type" : "module",
  "scripts" : {
    "prebuild" : "node generateBuildInfo.js",
    "prestart" : "node generateBuildInfo.js",
    "start" : "env-cmd -f .env.local vite",
    "build" : "tsc && env-cmd -f .env.prod vite build",
    "test" : "jest",
    "test:watch" : "jest --watch",
    "test:coverage" : "jest --coverage .",
    "test:debug" : "jest --inspect-brk --runInBand --no-cache",
    "preview" : "vite preview",
    "build:production" : "yarn prebuild && env-cmd -f .env.prod vite build"
  },
  "dependencies" : {
    "@ant-design/v5-patch-for-react-19" : "^1.0.3",
    "@testing-library/jest-dom" : "^6.6.3",
    "@types/jest" : "^29.5.14",
    "antd" : "^5.26.6",
    "axios" : "^1.7.9",
    "dayjs" : "^1.11.13",
    "env-cmd" : "^10.1.0",
    "react" : "^19.1.0",
    "react-dom" : "^19.1.0",
    "vempain-shared-auth" : "portal:/home/poltsi/Work/Vempain/vempain-frontend-auth"
  },
  "devDependencies" : {
    "@eslint/js" : "^9.30.1",
    "@testing-library/jest-dom" : "^6.6.3",
    "@types/jest" : "^29.5.14",
    "@types/react" : "^19.1.9",
    "@types/react-dom" : "^19.1.6",
    "@vitejs/plugin-react-swc" : "^3.10.2",
    "eslint" : "^9.30.1",
    "eslint-plugin-react-hooks" : "^5.2.0",
    "eslint-plugin-react-refresh" : "^0.4.20",
    "globals" : "^16.3.0",
    "jest" : "^29.7.0",
    "jest-environment-jsdom" : "^29.7.0",
    "ts-jest" : "^29.1.2",
    "typescript" : "~5.9.2",
    "typescript-eslint" : "^8.35.1",
    "vite" : "^7.0.4"
  },
  "eslintConfig" : {
    "extends" : [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest" : {
    "preset" : "ts-jest/presets/js-with-ts",
    "testEnvironment" : "jest-environment-jsdom",
    "moduleNameMapper" : {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$" : "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$" : "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "packageManager" : "yarn@4.9.2"
}
