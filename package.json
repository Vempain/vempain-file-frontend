{
  "name" : "@vempain/vempain-file-frontend",
  "type" : "module",
  "version" : "0.9.0",
  "description" : "Vempain File frontend",
  "author" : "Paul-Erik Törrönen",
  "homepage" : "https://vempain.poltsi.fi",
  "license" : "GPL-2.0",
  "packageManager" : "yarn@4.12.0",
  "scripts" : {
    "prebuild" : "node generateBuildInfo.js",
    "prestart" : "node generateBuildInfo.js",
    "start" : "env-cmd -f .env.local vite",
    "build" : "yarn prebuild && tsc && env-cmd -f .env.prod vite build",
    "test" : "jest",
    "test:watch" : "jest --watch",
    "test:coverage" : "jest --coverage .",
    "test:debug" : "jest --inspect-brk --runInBand --no-cache",
    "preview" : "vite preview",
    "build:production" : "yarn prebuild && env-cmd -f .env.prod vite build"
  },
  "dependencies" : {
    "@testing-library/jest-dom" : "^6.9.1",
    "@types/jest" : "^30.0.0",
    "@vempain/vempain-auth-frontend" : "^1.0.0",
    "antd" : "^6.3.0",
    "axios" : "^1.13.5",
    "dayjs" : "^1.11.19",
    "env-cmd" : "^11.0.0",
    "i18next" : "^25.8.5",
    "i18next-http-backend" : "^3.0.2",
    "leaflet" : "^1.9.4",
    "react" : "^19.2.4",
    "react-dom" : "^19.2.4",
    "react-i18next" : "^16.5.4",
    "react-leaflet" : "5.0.0"
  },
  "devDependencies" : {
    "@eslint/js" : "^10.0.1",
    "@testing-library/jest-dom" : "^6.9.1",
    "@types/jest" : "^30.0.0",
    "@types/leaflet" : "^1.9.21",
    "@types/react" : "^19.2.14",
    "@types/react-dom" : "^19.2.3",
    "@vitejs/plugin-react-swc" : "^4.2.3",
    "eslint" : "^10.0.0",
    "eslint-plugin-react-hooks" : "^7.0.1",
    "eslint-plugin-react-refresh" : "^0.5.0",
    "globals" : "^17.3.0",
    "jest" : "^30.2.0",
    "jest-environment-jsdom" : "^30.2.0",
    "translation-check" : "^1.1.1",
    "ts-jest" : "^29.4.6",
    "typescript" : "^5.9.3",
    "typescript-eslint" : "^8.55.0",
    "vite" : "^7.3.1",
    "vite-tsconfig-paths" : "^6.1.1"
  },
  "eslintConfig" : {
    "extends" : [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest" : {
    "preset" : "ts-jest/presets/js-with-ts",
    "testEnvironment" : "jest-environment-jsdom",
    "moduleNameMapper" : {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$" : "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$" : "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository" : {
    "type" : "git",
    "url" : "git+https://github.com/Vempain/vempain-auth-frontend.git"
  }
}
